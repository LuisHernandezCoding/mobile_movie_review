# See https://help.github.com/articles/ignoring-files for more about ignoring files.
/backend/.env

# Ignore bundler config.
/backend/.bundle

# Ignore the default SQLite database.
/backend/db/*.sqlite3
/backend/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/backend/log/*
/backend!/log/.keep
/backend/tmp
/backend/storage/*

# Ignore pidfiles, but keep the directory.
/backend/tmp/pids/*
/backend!/tmp/pids/
/backend!/tmp/pids/.keep

# Ignore master key for decrypting credentials and more.
/backend/config/master.key

# Ignore node_modules
/frontend/node_modules

# Expo
/frontend/.expo/
/frontend/dist/
/frontend/web-build/

# Native
/frontend/*.orig.*
/frontend/*.jks
/frontend/*.p8
/frontend/*.p12
/frontend/*.key
/frontend/*.mobileprovision

# Metro
/frontend/.metro-health-check*

# debug
/frontend/npm-debug.*
/frontend/yarn-debug.*
/frontend/yarn-error.*

# macOS
/frontend/.DS_Store
/frontend/*.pem

# local env files
/frontend/.env*.local

# typescript
/frontend/*.tsbuildinfo
